#include <iostream>
using namespace std;

class Node {
public:
    string name;
    int price;
    Node* next;

    Node(string n, int p) {
        name = n;
        price = p;
        next = NULL;
    }
};

class LinkedList {
    Node* head;

public:
    LinkedList() {
        head = NULL;
    }

    void addTail(string n, int p) {
        Node* t = new Node(n, p);
        if (!head) {
            head = t;
            return;
        }
        Node* u = head;
        while (u->next) u = u->next;
        u->next = t;
    }

    void delFront() {
        if (!head) return;
        Node* t = head;
        head = head->next;
        delete t;
    }

    void findByName(string key) {
        Node* u = head;
        while (u) {
            if (u->name == key) {
                cout << u->name << " " << u->price << endl;
                return;
            }
            u = u->next;
        }
        cout << "Not Found" << endl;
    }

    void findByPos(int pos) {
        int idx = 1;
        Node* u = head;
        while (u) {
            if (idx == pos) {
                cout << u->name << " " << u->price << endl;
                return;
            }
            u = u->next;
            idx++;
        }
        cout << "Invalid" << endl;
    }

    void show() {
        Node* u = head;
        while (u) {
            cout << u->name << " " << u->price << endl;
            u = u->next;
        }
    }
};

int main() {
    LinkedList cart;

    cart.addTail("One", 11);
    cart.addTail("Two", 22);
    cart.addTail("Three", 33);

    cart.delFront();
    cart.findByName("Two");
    cart.findByPos(2);
    cart.show();
}
